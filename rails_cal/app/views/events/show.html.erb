<p id="notice"><%= notice %></p>

<p>
  <b>Activity Name:</b>
  <%= @event.activity.activity_name %>
</p>

<p>
  <b>Event Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Start at:</b>
  <%= @event.start_at %>
</p>

<p>
  <b>End at:</b>
  <%= @event.end_at %>
</p>

<% if current_user.id == @event.activity.user.id %>
	<%= link_to 'Edit', edit_event_path(@event) %> |
 <% end %>

<%= link_to 'Back to Calendar', calendar_path(@event.activity.id) %>
